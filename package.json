    "lint:ci": "npx stylelint \"**/*.scss\" && nx affected:lint --all"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.sotsiaalministeerium.ee/tehik-react/tehik-react-components.git"
  },
