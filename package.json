  "commitlint": {
    "extends": "@commitlint/config-conventional",
    "rules": {
      "subject-case": [
        1,
        "always",
        "start-case"
      ]
    }
  },
  "lint-staged": {
    "*.{css,scss}": "stylelint --fix",
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{js,jsx,ts,tsx,css,scss,json,md}": "prettier --write"
